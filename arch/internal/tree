CTL_ROOT # root directory
CTL_ENV  # environment

CTL_ROOT/bash # bash library
CTL_ROOT/arch # architecture doc

CTL_ROOT/test     # testing
CTL_ROOT/test/run # run tests

CTL_DOC_GEN         # scripts for generating documentation
CTL_DOC_GEN/doc     # script for generating all docs
CTL_DOC_GEN/tmp.awk # script for generating 1 template doc
CTL_DOC_GEN/env.awk # script for generating env doc
CTL_DOC_GEN/hdr     # doc file headers
CTL_DOC_GEN/hdr/arr # header for dynamic array doc

CTL_TMP       # templates
CTL_TMP/arr.h # dynamic array

CTL_LIB                # library code
CTL_LIB/include/util.h # utilities header
CTL_LIB/include/src.c  # utility source

CTL_MKTEST         # scripts for generating tests
CTL_MKTEST/arr     # script for generated dynamic array test
CTL_MKTEST/tab     # tables of data for test generators
CTL_MKTEST/tab/arr # table for dynamic array test generator

CTL_TEST          # tests directory
CTL_TEST/arr.c    # dynamic array test
CTL_TEST/do       # auto generated parts of test code
CTL_TEST/do/arr.c # auto generated tests for dynamic array
