[ CTL_ROOT ]
    |
    +-->[ misc files ]
    |
    +-->[ CTL_TEST ]
    |       |
    |       +-->[ test files ]
    |       |
    |       +-->[ do ]
    |             |
    |             +-->[ auto generated tests ]
    |
    +--->[ CTL_DOC_GEN ]
    |       |
    |       +--->[ doc generator scripts ]
    |       |
    |       +--->[ hdr ]
    |               |
    |               +-->[ doc file headers ]
    |
    +--->[ CTL_TMP ]
    |       |
    |       +--->[ templates ]
    |
    +--->[ CTL_LIB ]
    |       |
    |       +--->[ library code ]
    |
    +--->[ CTL_MKTEST ]
            |
            +--->[ test generator scripts ]
            |
            +--->[ tab ]
                    |
                    +-->[ tables for test generators ]
